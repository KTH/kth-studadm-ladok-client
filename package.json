{
  "name": "kth-studadm-ladok-client",
  "version": "1.0.0",
  "description": "A client for the Ladok REST api with some support for relation links",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "preversion": "npm run codecheck",
    "postversion": "git push && git push --tags",
