  "repository": {
    "type": "git",
    "url": "git+https://github.com/KTH/kth-studadm-ladok-client.git"
  },
  "devDependencies": {
    "typescript": "^2.8.1",
    "tslint": "^5.9.1",
    "tslint-config-standard": "^7.0.0"
  },
  "dependencies": {
    "@types/request-promise-native": "^1.0.14",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5"
  }
}
